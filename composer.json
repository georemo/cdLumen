{
    "name": "laravel/lumen",
    "description": "The Laravel Lumen Framework.",
    "keywords": ["framework", "laravel", "lumen"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "laravel/lumen-framework": "5.8.*",
        "league/fractal": "^0.17.0",
        "vlucas/phpdotenv": "^3.3"
    },
    "require-dev": {
        "flipbox/lumen-generator": "^5.6",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/",
            "CD\\App\\Actts\\Controllers\\": "app/CD/App/Actts/Controllers/",
            "CD\\App\\Actts\\Models\\": "app/CD/App/Actts/Models/",
            "CD\\App\\Hrm\\Controllers\\": "app/CD/App/Hrm/Controllers/",
            "CD\\App\\Hrm\\Models\\": "app/CD/App/Hrm/Models/",
            "CD\\Sys\\CDObj\\Controllers\\": "app/CD/App/CDObj/Controllers/",
            "CD\\Sys\\": "app/CD/Sys/",
            "CD\\Sys\\CDObj\\Models\\": "app/CD/Sys/CDObj/Models/",
            "CD\\Sys\\Comm\\Controllers\\": "app/CD/Sys/Comm/Controllers/",
            "CD\\Sys\\Comm\\Models\\": "app/CD/Sys/Comm/Models/",
            "CD\\Sys\\Docproc\\Controllers\\": "app/CD/Sys/Docproc/Controllers/",
            "CD\\Sys\\Docproc\\Models\\": "app/CD/Sys/Docproc/Models/",
            "CD\\Sys\\Moduleman\\Controllers\\": "app/CD/Sys/Moduleman/Controllers/",
            "CD\\Sys\\Moduleman\\Models\\": "app/CD/Sys/Moduleman/Models/",
            "CD\\Sys\\Scheduler\\Controllers\\": "app/CD/Sys/Scheduler/Controllers/",
            "CD\\Sys\\Scheduler\\Models\\": "app/CD/Sys/Scheduler/Models/",
            "CD\\Sys\\User\\Controllers\\": "app/CD/Sys/User/Controllers/",
            "CD\\Sys\\User\\Models\\": "app/CD/Sys/User/Models/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ],
        "psr-4": {
            "Tests\\": "tests/",
            "CD\\App\\Actts\\Test\\": "app/CD/App/Actts/Test/",
            "CD\\App\\Hrm\\Test\\": "app/CD/App/Hrm/Test/",
            "CD\\Sys\\CDObj\\Test\\": "app/CD/Sys/CDObj/Test/",
            "CD\\Sys\\Comm\\Test\\": "app/CD/Sys/Comm/Test/",
            "CD\\Sys\\Docproc\\Test\\": "app/CD/Sys/Docproc/Test/",
            "CD\\Sys\\Moduleman\\Test\\": "app/CD/Sys/Moduleman/Test/",
            "CD\\Sys\\Scheduler\\Test\\": "app/CD/Sys/Scheduler/Test/",
            "CD\\Sys\\User\\Test\\": "app/CD/Sys/User/Test/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
